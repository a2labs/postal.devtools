{
  "name": "Postal Inspector",
  "version": "0.1.3",
  "manifest_version": 2,
  "minimum_chrome_version": "18.0",
  "devtools_page": "devtools.html",
  "background": {
    "scripts": [ "background.js" ]
  },
  "content_scripts": [
      {
      	"run_at": "document_start",
        "matches": ["http://*/*"],
        "js": ["postal.bridge.js"]
      }
  ]
}